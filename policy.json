{
        "Version": "2008-10-17",
        "Id": "S3PolicyId1",
        "Statement": [
                {
                        "Sid": "PutObject",
                        "Effect": "Allow",
                        "Principal": {
                                "AWS": "*"
                        },
                        "Action": "s3:PutObject",
                        "Resource": "arn:aws:s3:::themode_server3_bakup/*"
                },
                {
                        "Sid": "IPDeny",
                        "Effect": "Deny",
                        "Principal": {
                                "AWS": "*"
                        },
                        "Action": "s3:GetObject",
                        "Resource": "arn:aws:s3:::themode_server3_bakup/*",
                        "Condition": {
                                "NotIpAddress": {
                                        "aws:SourceIp": ["115.110.117.254/32","14.141.213.170/32"]
                                }
                        }
                }
        ]
}